<Window x:Class="MoonScore.Views.PieChartDataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoonScore.Views" xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="PieChartDataWindow" Height="500" Width="800">

	<Window.Background>
		<ImageBrush ImageSource="pack://application:,,,/Assets/Images/Backgrounds\background_2.jpeg" Stretch="UniformToFill"/>
	</Window.Background>

	<Grid>
		<oxy:PlotView x:Name="MoonPhasePlot" Height="400" Width="700">
			<!--<oxy:PlotView.Background>
				Transparent
			</oxy:PlotView.Background>-->
		</oxy:PlotView>
	</Grid>
</Window>
