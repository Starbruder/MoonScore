<Window x:Class="MoonScore.Views.APIScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoonScore.Views"
        mc:Ignorable="d"
        Title="MoonScore" Height="900" Width="1600"
        Style="{StaticResource MaterialDesignWindow}"
        WindowStartupLocation="CenterScreen"
        Icon="/Views/moonscore_IconLogo.png">

    <Window.Background>
        <ImageBrush ImageSource="pack://application:,,,/Assets/Images/Backgrounds\background_4.png" Stretch="UniformToFill"/>
    </Window.Background>
    <Grid>
        <Button Content="Back to MainScreen" Width="200" Height="50" Margin="2, 2, 2, 2" Click="OpenMainWindow"/>
    </Grid>
</Window>
